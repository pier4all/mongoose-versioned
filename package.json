{
    "name": "mongoose-versioned",
    "version": "1.3.0",
    "description": "Versioning module for MongoDB based in mongoose. It uses a main collection for the current data and a shadow collection for the old versions.",
    "main": "dist/versioning.js",
    "type": "module",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/pier4all/mongoose-versioned"
    },
    "bugs": {
        "url": "https://github.com/pier4all/mongoose-versioned/issues"
    },
    "homepage": "https://github.com/pier4all/mongoose-versioned#readme",
    "keywords": [
        "mongoose",
        "mongodb",
        "versioning"
    ],
    "scripts": {
        "test": "tap --node-arg=--require=ts-node/register test/*.test.ts",
        "preinstall": "npm install --package-lock-only --ignore-scripts && npx npm-force-resolutions",
        "build": "tsc"
    },
    "author": {
        "name": "Lucía de Espona Pernas",
        "url": "https://github.com/espona"
    },
    "contributors": [
        {
            "name": "Lucía de Espona Pernas",
            "url": "https://github.com/espona"
        },
        {
            "name": "Jean-Claude Schmidig",
            "url": "https://github.com/jcschmidig"
        }
    ],
    "license": "MIT",
    "dependencies": {
        "immutable": "^4.0.0-rc.12",
        "mongoose": "^6.2.2"
    },
    "devDependencies": {
        "chalk": "^5.2.0",
        "mongodb-memory-server": "^8.0.4",
        "tap": "^16.3.4",
        "ts-node": "^10.9.1",
        "typescript": "^4.9.5"
    },
    "peerDependencies": {
        "mongodb": "^5.1.0"
    },
    "resolutions": {
        "minimist": "^1.2.6"
    }
}
